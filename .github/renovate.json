{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prConcurrentLimit": 4,
  "includePaths": [
    "package.json"
  ],
  "rangeStrategy": "update-lockfile",
  "ignoreUnstable": false,
  "respectLatest": false,
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "enabled": false
    },
    {
      "matchPackageNames": [
        "testcafe"
      ],
      "allowedVersions": "<13",
      "ignoreUnstable": false,
      "respectLatest": false,
      "enabled": true
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": false
  },
  "branchPrefix": "update-testcafe-renovate-bot/",
  "automerge": false
}
